{
  "name": "Toolkit Search",
  "version": "1.0",
  "description": "Search Netcompany Toolkit in omnibox",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "https://goto.netcompany.com/cases/*/_api/*"
  ],
  "browser_action": {
    "default_title": "Toolkit Search",
    "default_popup": "options.html"
  },
  "omnibox": {
    "keyword": "tk"
  },
  "manifest_version": 2
}
