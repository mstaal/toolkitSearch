{
  "name": "Toolkit Search",
  "version": "1.1.1",
  "description": "Search Netcompany SharePoint Toolkit in omnibox",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "https://goto.netcompany.com/cases/*/_api/*"
  ],
  "browser_action": {
    "default_title": "Toolkit Search",
    "default_popup": "options.html"
  },
  "omnibox": {
    "keyword": "tk"
  },
  "manifest_version": 2
}
